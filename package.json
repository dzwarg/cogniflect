{
  "name": "cogniflect",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "concurrently": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "merge": ">=1.2.1",
    "nodemon": "^1.19.4",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.10.2",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "3.0.9",
    "react-test-renderer": "^16.10.2"
  },
  "scripts": {
    "build": "concurrently \"npm:build-*\"",
    "build-socketserver": "babel src/lib -d dist -s --ignore \"src/**/*.test.js\"",
    "build-webapp": "react-scripts build",
    "check-updates": "ncu",
    "coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "start": "concurrently \"npm:start-*\"",
    "start-socketserver": "nodemon -w dist/server.js dist/index.js",
    "start-webapp": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "proxy": "http://localhost:5000/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run coverage"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
